<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
  <properties resource="jeoneunhye/vms/conf/jdbc.properties"></properties>

  <typeAliases>
    <typeAlias type="jeoneunhye.vms.domain.Video" alias="Video"/>
    <typeAlias type="jeoneunhye.vms.domain.Member" alias="Member"/>
    <typeAlias type="jeoneunhye.vms.domain.Board" alias="Board"/>
    <typeAlias type="jeoneunhye.vms.domain.PhotoBoard" alias="PhotoBoard"/>
    <typeAlias type="jeoneunhye.vms.domain.PhotoFile" alias="PhotoFile"/>
  </typeAliases>

  <environments default="development">
    <environment id="development">
      <transactionManager type="JDBC"/>
      <dataSource type="POOLED">
        <property name="driver" value="${jdbc.driver}"/>
        <property name="url" value="${jdbc.url}"/>
        <property name="username" value="${jdbc.username}"/>
        <property name="password" value="${jdbc.password}"/>
      </dataSource>
    </environment>
  </environments>

  <mappers>
    <mapper resource="jeoneunhye/vms/mapper/VideoMapper.xml"/>
    <mapper resource="jeoneunhye/vms/mapper/MemberMapper.xml"/>
    <mapper resource="jeoneunhye/vms/mapper/BoardMapper.xml"/>
    <mapper resource="jeoneunhye/vms/mapper/PhotoBoardMapper.xml"/>
    <mapper resource="jeoneunhye/vms/mapper/PhotoFileMapper.xml"/>
  </mappers>
</configuration>